<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andteam.sep4greenhouse">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:vmSafeMode="true">
        <activity android:name="com.andteam.sep4greenhouse.view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.andteam.sep4greenhouse.view.MainActivity"
            android:exported="true"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.ModifyPlantProfileActivity"
            android:exported="true"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.RegisterActivity"
            android:exported="true"></activity>
        <activity android:name="com.andteam.sep4greenhouse.view.ViewPlantProfileActivity"
            android:exported="true"></activity>
    </application>
</manifest>